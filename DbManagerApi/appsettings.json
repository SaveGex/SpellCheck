{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JWT": {
    "KeyWord": "DbManagerApiKey",
    "Issuer": "DbManagerApi",
    "Audience": "DbManagerApiUser",
    "AccessTokenExpirationMinutes": 60,
    "RefreshTokenExpirationDays": 7
  },
  "CacheSettings": {
    "ClientCacheExpirationTimeInMinutes" : 60
  }
}
